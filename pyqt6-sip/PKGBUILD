
pkgname=pyqt6-sip
_pkgname=PyQt6_sip
pkgver=13.2.0
pkgrel=1
arch=('x86_64')
pkgdesc="The sip extension module provides support for the PyQt6 package."
url="https://riverbankcomputing.com/software/pyqt/intro"
license=('GPL')
depends=('python3')
makedepends=('python3-setuptools')
source=("https://pypi.python.org/packages/source/P/PyQt6-sip/${_pkgname}-${pkgver}.tar.gz")
sha256sums=('10e6c5d0004308efcbe8826ff0b8faf6e6f60c9e906cffeb26834e18e6340ed4')

build() {
  cd ${_pkgname}-${pkgver}

  python3 setup.py build
}

package()  {
  cd ${_pkgname}-${pkgver}

  python3 setup.py install --root=${pkgdir}
}
