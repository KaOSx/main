
pkgname=aspell-cs
pkgver=20040614.1
_pkgver=20040614-1
pkgrel=1
pkgdesc="Czech dictionary for aspell"
arch=('x86_64')
url="http://aspell.net/"
license=('GPL')
depends=('aspell')
source=("https://ftp.gnu.org/gnu/aspell/dict/cs/aspell6-cs-${_pkgver}.tar.bz2")
md5sums=('50f0c2b7b6fcfe47bb647ad8993d2fe8')

build() {
  cd aspell6-cs-${_pkgver}

  ./configure
  make
}

package() {
  cd aspell6-cs-${_pkgver}

  make DESTDIR=${pkgdir} install
}
