
pkgname=python3-cython
pkgver=0.29.30
pkgrel=1
arch=('x86_64')
url="https://cython.org/"
license=('APACHE')
pkgdesc="C-Extensions for Python 3"
depends=('python3')
makedepends=('python3-setuptools')
#source=("https://github.com/cython/cython/releases/download/${pkgver}/Cython-${pkgver}.tar.gz")
source=("https://files.pythonhosted.org/packages/source/C/Cython/Cython-${pkgver}.tar.gz")
sha256sums=('2235b62da8fe6fa8b99422c8e583f2fb95e143867d337b5c75e4b9a1a865f9e3')

package() {
  cd Cython-${pkgver}

  python3 setup.py install --root=${pkgdir}
}
