
pkgname=python2-pyparsing
pkgver=2.2.0
pkgrel=1
pkgdesc='General parsing module for Python'
arch=('x86_64')
url='http://pyparsing.wikispaces.com'
license=('MIT')
depends=('python2')
makedepends=('python2-setuptools')
source=("https://downloads.sourceforge.net/pyparsing/pyparsing-${pkgver}.tar.gz")
md5sums=('0214e42d63af850256962b6744c948d9')

package() {
  cd pyparsing-${pkgver}
  rm -rf build
  
  python2 setup.py install --prefix=/usr --root=${pkgdir} --optimize=1
  install -Dm644 LICENSE ${pkgdir}/usr/share/licenses/python2-pyparsing/LICENSE
}

