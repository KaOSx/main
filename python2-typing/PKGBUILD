 
pkgname=python2-typing
pkgver=3.6.6
pkgrel=1
pkgdesc="Backport of the standard library typing module to Python versions older than 3.5"
url="https://docs.python.org/3/library/typing.html"
arch=('x86_64')
license=('PSF')
makedepends=('python2-setuptools')
source=("https://pypi.io/packages/source/t/typing/typing-${pkgver}.tar.gz")
sha256sums=('4027c5f6127a6267a435201981ba156de91ad0d1d98e9ddc2aa173453453492d')

build() {
    cd typing-${pkgver}
    
    python2 setup.py build
}

package() {
	cd typing-${pkgver}
	
	python2 setup.py install --root ${pkgdir} --optimize=1 --skip-build
	install -Dm644 LICENSE ${pkgdir}/usr/share/licenses/${pkgname}/LICENSE
}
