
pkgname=mutagen
pkgver=1.42.0
pkgrel=1
pkgdesc="An audio metadata tag reader and writer implemented in pure python"
arch=('x86_64')
url="https://mutagen.readthedocs.org/en/latest/"
license=('GPL2')
depends=('python2-setuptools')
source=("https://github.com/quodlibet/mutagen/releases/download/release-${pkgver}/${pkgname}-${pkgver}.tar.gz")
sha256sums=('bb61e2456f59a9a4a259fbc08def6d01ba45a42da8eeaa97d00633b0ec5de71c')

package() {
  cd ${pkgname}-${pkgver}
  
  python2 setup.py install --root=${pkgdir}
}
