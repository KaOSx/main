
pkgname=python3-gdal
_pkgname=GDAL
pkgver=2.1.3
pkgrel=1
pkgdesc="Geospatial Data Abstraction Library- python3 support"
arch=('x86_64')
url="https://pypi.python.org/pypi/GDAL/"
license=('MIT')
depends=('python3' 'gdal')
source=("https://pypi.python.org/packages/36/d7/f89ac1347185db56939c156330efbfa2e3be560060b74e31f41e514ee627/${_pkgname}-${pkgver}.tar.gz")
md5sums=('519bff3c56b8af5dbf388f93947b1281')

build() {
  cd ${_pkgname}-${pkgver}
  
  python3 setup.py build
}

package() {
  cd ${_pkgname}-${pkgver}
  
  python3 setup.py install --skip-build --prefix=/usr --root=${pkgdir}
}
